{
  "name": "react-infi-scroller",
  "version": "1.1.1",
  "description": "React-based infinite scroll component",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "dependencies": {
    "get-node-dimensions": "^1.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/get-node-dimensions": "^1.2.0",
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.9.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^25.1.0",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^1.29.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-watch": "^4.3.1",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "rollup -c --watch",
    "build": "rollup -c",
    "test": "jest"
  },
  "keywords": [
    "react",
    "infinite-scroll",
    "typescript",
    "prop-types"
  ],
  "jest": {
    "rootDir": "./src",
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jsdom",
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup/jest.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/assets",
      "<rootDir>/__tests__/declarations",
      "<rootDir>/__tests__/setup"
    ]
  },
  "author": "Neven Dyulgerov",
  "license": "MIT"
}
